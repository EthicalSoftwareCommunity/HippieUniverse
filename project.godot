; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="HippieUniverse"
run/main_scene="res://HippieFall/Sourse/HippieFall.tscn"
config/use_hidden_project_data_directory=false
config/icon="res://icon.png"
config/auto_accept_quit=false

[compression]

formats/zstd/long_distance_matching=true

[debug]

settings/stdout/print_fps=true
settings/gdscript/max_call_stack=4096

[display]

window/size/test_width=1024
window/size/test_height=600
window/vsync/use_vsync=false
window/energy_saving/keep_screen_on=false
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray(  )

[global]

debug=true

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input]

move_to_cursor={
"deadzone": 0.5,
"events": [ Object(InputEventMouseButton,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"button_mask":0,"position":Vector2( 0, 0 ),"global_position":Vector2( 0, 0 ),"factor":1.0,"button_index":1,"pressed":false,"doubleclick":false,"script":null)
 ]
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

3d_physics/layer_1="Wall"
3d_physics/layer_4="Destroy Trigger"
3d_physics/layer_5="PlayerTriggers"
3d_physics/layer_8="Collectable"

[logging]

file_logging/enable_file_logging=true
file_logging/max_log_files=50

[mono]

runtime/unhandled_exception_policy=1
project/assembly_name="HippieUniverse"
debugger_agent/wait_timeout=8216

[physics]

common/enable_pause_aware_picking=true
3d/physics_engine="GodotPhysics"
3d/active_soft_world=false

[rendering]

quality/directional_shadow/size=256
quality/directional_shadow/size.mobile=256
quality/shadow_atlas/size=256
quality/shadow_atlas/size.mobile=256
quality/shadow_atlas/cubemap_size=64
quality/shadow_atlas/quadrant_0_subdiv=0
quality/shadow_atlas/quadrant_1_subdiv=0
quality/shadow_atlas/quadrant_2_subdiv=0
quality/shadow_atlas/quadrant_3_subdiv=0
quality/shadows/filter_mode=0
quality/reflections/high_quality_ggx=false
quality/subsurface_scattering/quality=0
quality/subsurface_scattering/follow_surface=true
quality/reflections/atlas_size=512
quality/depth/hdr=false
environment/default_environment="res://default_env.tres"
